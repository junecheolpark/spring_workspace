package com.kh.intro;

public class Run {
	public static void main(String[] args) {
		//우리회사가 samsung 계약
				//laptop 생산
				SamsungLaptop samsung = new SamsungLaptop();
				samsung.powerOn();
				samsung.powerOff();
				samsung.volumeUp();
				samsung.volumeDown();
				
				//삼성이 아닌 lg랑 계약
				//latop 생산
				LgLaptop lg = new LgLaptop();
				lg.powerOn();
				lg.powerOff();
				lg.volumeUp();
				lg.volumeDown();
	}

}
